{
  "manifest_version": 3,
  "author": "robin.on.branch@gmail.com",
  "name": "Cetacension",
  "description": "A chrome extension for dolphins on the internet",
  "version": "1.0",
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["activeTab", "scripting"]
}
